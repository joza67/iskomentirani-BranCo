{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.1.3",
      "destination": "wwwroot/lib/bootstrap/"
    },

    {
      "library": "font-awesome@5.11.2",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "jquery-validate@1.19.1",
      "destination": "wwwroot/lib/jquery-validate/"
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "library": "leaflet@1.3.2",
      "destination": "wwwroot/lib/leaflet/"
    },
    {
      "library": "leaflet.fullscreen@2.2.0",
      "destination": "wwwroot/lib/leaflet/"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.min.js.map",
        "dist/browser/signalr.js.map"
      ]
    },
    {
      "library": "popper.js@2.11.0",
      "destination": "wwwroot/lib/popper.js/"
    },
    {
      "library": "bootstrap-datepicker@1.9.0",
      "destination": "wwwroot/lib/bootstrap-datepicker/"
    }
  ]
}